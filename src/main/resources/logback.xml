<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%level][%date{ISO8601}][%logger{16}] %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="lambda" level="DEBUG"/>

    <root level="WARN">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>